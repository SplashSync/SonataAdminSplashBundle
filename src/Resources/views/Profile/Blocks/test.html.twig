
    <div class="box box-primary">
        <div class="box-body ">
            <div class="info-box">

                {% if result | default(false) %}
                    <span class="info-box-icon bg-green-gradient">
                        <i class="fa fa-check-circle text-default" aria-hidden="true"></i>
                    </span>
                {% else %}
                    <span class="info-box-icon bg-red-gradient">
                        <i class="fa fa-times-circle text-default" aria-hidden="true"></i>
                    </span>
                {% endif %}                

                <div class="info-box-content">

                    <span class="info-box-text">{{ title }}</span>

                    <div class="progress">
                        <div class="progress-bar" style="width: 00%"></div>
                    </div>
                    
                    <span class="progress-description">
                        {{ log | raw }}
                    </span>
                    
                </div>
            </div>
        </div>
    </div>

